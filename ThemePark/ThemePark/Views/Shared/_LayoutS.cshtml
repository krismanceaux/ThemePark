

<!DOCTYPE html>
<style>

    section {
        padding: 60px 0;
    }

        section .section-title {
            text-align: center;
            margin-bottom: 50px;
            text-transform: uppercase;
        }

    #footer {
        background: #FFFFFF !important;
    }

        #footer h5 {
            padding-left: 10px;
            border-left: 3px solid #eeeeee;
            padding-bottom: 6px;
            margin-bottom: 20px;
        }

        #footer a {
            text-decoration: none !important;
            background-color: transparent;
            -webkit-text-decoration-skip: objects;
        }

        #footer ul.social li {
            padding: 3px 0;
        }

            #footer ul.social li a i {
                margin-right: 5px;
                font-size: 25px;
                -webkit-transition: .5s all ease;
                -moz-transition: .5s all ease;
                transition: .5s all ease;
            }

            #footer ul.social li:hover a i {
                font-size: 30px;
                margin-top: -10px;
            }

            #footer ul.social li a,
            #footer ul.quick-links li a {
            }

                #footer ul.social li a:hover {
                }

        #footer ul.quick-links li {
            padding: 3px 0;
            -webkit-transition: .5s all ease;
            -moz-transition: .5s all ease;
            transition: .5s all ease;
        }

            #footer ul.quick-links li:hover {
                padding: 3px 0;
                margin-left: 5px;
                font-weight: 700;
            }

            #footer ul.quick-links li a i {
                margin-right: 5px;
            }

            #footer ul.quick-links li:hover a i {
                font-weight: 700;
            }

    a.my-class {
        color: #C71585
    }

        a.my-class:active {
            color: #C71585
        }

        a.my-class:link {
            color: #C71585
        }

        a.my-class:visited {
            color: #C71585
        }

    a.my-class2 {
        color: #008080
    }

        a.my-class2:active {
            color: #008080
        }

        a.my-class2:link {
            color: #008080
        }

        a.my-class2:visited {
            color: #008080
        }

    a.my-class3 {
        color: #FFA500
    }

        a.my-class3:active {
            color: #FFA500
        }

        a.my-class3:link {
            color: #FFA500
        }

        a.my-class3:visited {
            color: #FFA500
        }


    a.my-class4 {
        color: #00CED1
    }

        a.my-class4:active {
            color: #00CED1
        }

        a.my-class4:link {
            color: #00CED1
        }

        a.my-class4:visited {
            color: #00CED1
        }

    a.my-class5 {
        color: #0000cd
    }

        a.my-class5:active {
            color: #0000cd
        }

        a.my-class5:link {
            color: #0000cd
        }

        a.my-class5:visited {
            color: #0000cd
        }
    }
</style>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="https://fonts.googleapis.com/css?family=Rokkitt:400,500,700" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a href="@Url.Action("IndexS", "Home")" class="navbar-brand"><i class="fas fa-home"></i></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" style="color:#C71585" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        THINGS TO DO
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        @Html.ActionLink("TEXAS Coaster", "TEXAS_CoasterS", "ThingsToDo", null, new { @class = "nav-link" })
                        @Html.ActionLink("UH Ride", "UH_rideS", "ThingsToDo", null, new { @class = "nav-link" })
                        @Html.ActionLink("CS Train ", "CS_TrainS", "ThingsToDo", null, new { @class = "nav-link" })
                        @Html.ActionLink("DBMS Drive ", "DBMS_DriveS", "ThingsToDo", null, new { @class = "nav-link" })

                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" style="color:#008080" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        PLAN VISIT
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        @Html.ActionLink("Park Map", "Park_MapS", "PlanVisit", null, new { @class = "nav-link" })
                        @Html.ActionLink("Event Calendar", "CalendarS", "PlanVisit", null, new { @class = "nav-link" })

                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" style="color:#FFA500" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        SHOP
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        @Html.ActionLink("Food", "FoodS", "Shop", null, new { @class = "nav-link" })
                        @Html.ActionLink("Drink", "DrinkS", "Shop", null, new { @class = "nav-link" })
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" style="color:#00CED1" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        PARK INFORMATION
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        @Html.ActionLink("About Us", "AboutUsS", "ParkInformation", null, new { @class = "nav-link" })
                        @Html.ActionLink("Social Media", "SocialMediaS", "ParkInformation", null, new { @class = "nav-link" })
                    </div>
                </li>
            </ul>
            <ul class="navbar-nav ml-auto">
                <li>@Html.ActionLink("Logout", "Index", "Home", new { id = 1 }, new { @class = "nav-link ml-auto" })</li>
            </ul>
        </div>

    </nav>
    <div class="body-content">
        @RenderBody()
        <hr />

    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>





<section id="footer">
    <div class="container">
        <div class="row text-center text-xs-center text-sm-left text-md-left">
            <div class="col-xs-12 col-sm-4 col-md-4" style="color: #C71585">

                <h5>Things to Do</h5>
                <ul class="list-unstyled quick-links" style="color: #C71585">
                    <li>@Html.ActionLink("TEXAS Coaster", "TEXAS_CoasterS", "ThingsToDo", null, new { @class = "my-class" })</li>
                    <li>@Html.ActionLink("UH Ride", "UH_rideS", "ThingsToDo", null, new { @class = "my-class" })</li>
                    <li>@Html.ActionLink("CS Train ", "CS_TrainS", "ThingsToDo", null, new { @class = "my-class" })</li>
                    <li>@Html.ActionLink("DBMS Drive ", "DBMS_DriveS", "ThingsToDo", null, new { @class = "my-class" })</li>
                </ul>
            </div>
            <div class="col-xs-12 col-sm-4 col-md-4" style="color: #008080">
                <h5>Plan Visit</h5>
                <ul class="list-unstyled quick-links" style="color: #008080">
                    <li>
                        @Html.ActionLink("Park Map", "Park_MapS", "PlanVisit", null, new { @class = "my-class2" })

                    </li>
                    <li>@Html.ActionLink("Event Calendar", "CalendarS", "PlanVisit", null, new { @class = "my-class2" })</li>

                </ul>
            </div>
            <div class="col-xs-12 col-sm-4 col-md-4" style="color: #FFA500">
                <h5>Shop</h5>
                <ul class="list-unstyled quick-links" style="color: #FFA500">
                    <li>
                        @Html.ActionLink("Food", "FoodS", "Shop", null, new { @class = "my-class3" })

                    </li>
                    <li> @Html.ActionLink("Drink", "DrinkS", "Shop", null, new { @class = "my-class3" })</li>

                </ul>
            </div>
            </br>
            <div class="col-xs-12 col-sm-4 col-md-4" style="color: #00CED1">
                <h5>Park information</h5>
                <ul class="list-unstyled quick-links" style="color: #00CED1">
                    <li>
                        @Html.ActionLink("About Us", "AboutUsS", "ParkInformation", null, new { @class = "my-class4" })

                    </li>
                    <li>@Html.ActionLink("Social Media", "SocialMediaS", "ParkInformation", null, new { @class = "my-class4" })</li>

                </ul>
            </div>

            <div class="col-xs-12 col-sm-4 col-md-4" style="color: #0000cd">
                <h5>Login</h5>
                <ul class="list-unstyled quick-links" style="color: #0000cd">

                    <li>@Html.ActionLink("Pass Holder Login", "Login", "SPHAccount", null, new { @class = "my-class5" })</li>
                    <li>@Html.ActionLink("Employee Login", "Login", "EmployeeAccount", null, new { @class = "my-class5" })</li>

                </ul>
            </div>

        </div>

        


       
    </div>
</section>


</html>
